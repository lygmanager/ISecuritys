<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RightAndLeftTextView">
        <attr name="left_text" format="reference|string"/>
        <attr name="left_color" format="color"/>
        <attr name="left_size" format="dimension"/>
        <attr name="right_text" format="reference|string"/>
        <attr name="right_color" format="color"/>
        <attr name="right_size" format="dimension"/>
        <attr name="wrapcontent" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="RightAndLeftEditText">
        <!-- 标题的大小 -->
        <attr name="nameSize" format="dimension" />
        <!-- 标题的内容 -->
        <attr name="nameText" format = "reference|string" />
        <!-- 编辑框选中的颜色 -->
        <attr name="normalColor" format="color" />
        <!-- 编辑框没选中的颜色 -->
        <attr name="selcetColor" format="color" />

        <attr name="editAble" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CompoundButtonGroup">
        <attr name="entries" format="reference" />
        <attr name="numCols" format="integer" />
        <attr name="labelOrder" format="enum">
            <enum name="before" value="0" />
            <enum name="after" value="1" />
        </attr>
        <attr name="compoundType" format="enum">
            <enum name="check_box" value="0" />
            <enum name="radio" value="1" />
        </attr>
    </declare-styleable>
</resources>